{% extends "base.twig" %}

{% block content %}
<div class="home-content">
	<h1>{{home_title}}</h1>
	{{home_content}}
</div>
{% if include_feed %}
	{{feed_content}}
{% else %}
<div class="home-images">
	{% for image in home_images %}
		<img src="{{image.home_image.url}}" alt="{{image.home_image.alt}}">
	{% endfor %}
</div>
{% endif %}
{% endblock %}
